<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="po.FixRecord" table="fix_record" catalog="toolmanagesys">
        <id name="eID" type="string">
            <column name="EID" length="30"/>
        </id>
        <property name="applyUID" type="int">
            <column name="Apply_UID"/>
        </property>
        <property name="dealUID" type="int">
            <column name="Deal_UID" />
        </property>
        <property name="description" type="string">
            <column name="Description" length="30"/>
        </property>
        <property name="img" type="string">
            <column name="Img" length="45"/>
        </property>
        <property name="seqID" type="int">
            <column name="SeqID" />
        </property>
    </class>
</hibernate-mapping>