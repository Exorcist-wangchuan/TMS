<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="po.DefineTool" table="define_tool" catalog="toolmanagesys">
        <id name="id" type="int">
            <column name="ID"/>
        </id>
        <property name="code" type="string">
            <column name="Code" length="10"/>
        </property>
        <property name="name" type="string">
            <column name="Name" length="30"/>
        </property>
        <property name="familyID" type="int">
            <column name="FamilyID" />
        </property>
        <property name="family" type="string">
            <column name="Family" length="20"/>
        </property>
        <property name="model" type="string">
            <column name="Model" length="20"/>
        </property>
        <property name="partNo" type="string">
            <column name="PartNo" length="20"/>
        </property>
        <property name="upl" type="int">
            <column name="UPL" />
        </property>
        <property name="usedFor" type="string">
            <column name="UsedFor" length="30"/>
        </property>
        <property name="pmPeriod" type="int">
            <column name="PMPeriod" />
        </property>
        <property name="owner" type="int">
            <column name="Owner" />
        </property>
        <property name="ownerName" type="string">
            <column name="OwnerName" length="10"/>
        </property>
        <property name="recOn" type="string">
            <column name="RecOn" length="20"/>
        </property>
        <property name="recBy" type="int">
            <column name="RecBy" />
        </property>
        <property name="recByName" type="string">
            <column name="RecByName" length="10"/>
        </property>
        <property name="editOn" type="string">
            <column name="EditOn" length="20"/>
        </property>
        <property name="editBy" type="int">
            <column name="EditBy" />
        </property>
        <property name="editByName" type="string">
            <column name="EditByName" length="10"/>
        </property>
        <property name="remark" type="string">
            <column name="Remark" length="20"/>
        </property>
        <property name="workCellID" type="int">
            <column name="WorkCellID" />
        </property>
        <property name="workCell" type="string">
            <column name="WorkCell" length="10"/>
        </property>
    </class>
</hibernate-mapping>